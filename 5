programa
{
	funcao inicio()
	{
		inteiro valor[10] 

		escreva("Digite números inteiros: \n")
		para(inteiro i = 0; i < 10; i++){
			leia(valor[i])
			
			se (valor[i] %2 == 0){
           	escreva("A posição ", i , " apresenta um número par \n")
           	}
          
          
           }

	}
}
