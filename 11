programa 
{
	funcao inicio() {
		
		cadeia procura[3][3] = {
		    {"Walso", "Vila", "Volley"},
		    {"Wilson", "Valdo", "Vasco"},
		    {"Wallace", "Willy", "Wally"}
		}
		
		para(inteiro i = 0; i< 3; i++){
		    para(inteiro c = 0; c< 3; c++){
		        se(procura[i][c] == "Wally"){
		            escreva(" Wally está na matriz. Está na linha ", i + 1, " na coluna ", c + 1)
		        }
		    }
		}
	}
}
