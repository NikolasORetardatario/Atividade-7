programa 
{
	funcao inicio() {
        
        inteiro numeros[3][3] = {
            {2,4,6},
            {8,10,12},
            {14,16,18}
        }
        
        para(inteiro i = 0; i< 3; i++){
            para(inteiro c = 0; c < 3; c++){
                escreva(numeros[i][c], " ")
            }
	    
            	escreva("\n")
	    
        }
	
	}
}
